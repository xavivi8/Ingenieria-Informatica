  FormBox[
   SuperscriptBox["y", 
    RowBox[{
     RowBox[{"n", "-", "1"}], ")"}]], TraditionalForm]],ExpressionUUID->
  "c0a3c469-955e-4468-ab98-ef084ebd6156"],
 "+...",
 Cell[BoxData[
  FormBox[
   RowBox[{"+", 
    SubscriptBox["a", "1"]}], TraditionalForm]],ExpressionUUID->
  "75ca6fe6-9455-433a-b287-f33d8b84eaa3"],
 " ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["y", "'"], TraditionalForm]],ExpressionUUID->
  "04240e82-d69c-470e-89de-875013032c82"],
 "+",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "0"], TraditionalForm]],ExpressionUUID->
  "05288032-e1f7-4408-915b-daea25cb5c2e"],
 " y =0\n6) ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "n"], TraditionalForm]],ExpressionUUID->
  "408c609a-cb65-4040-8628-a48f4bc60d3c"],
 " ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["y", 
    RowBox[{"n", ")"}]], TraditionalForm]],ExpressionUUID->
  "e41c6f62-1fb7-44ae-9aa6-a20f4f7ccc48"],
 "+",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", 
    RowBox[{"n", "-", "1"}]], TraditionalForm]],ExpressionUUID->
  "920035aa-fb9f-4a1c-85ed-21a12df5cf64"],
