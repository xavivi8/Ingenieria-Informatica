       RowBox[{"\[ImaginaryI]", " ", 
        RowBox[{"sen", "(", 
         FractionBox["\[Pi]", "2"], ")"}]}]}], ")"}]}], "-", " ", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{"-", 
      FractionBox["\[Pi]", "2"]}]], "-", 
    RowBox[{"\[ImaginaryI]", " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"cos", "(", 
        RowBox[{"-", 
         FractionBox["\[Pi]", "2"]}], ")"}], "+", 
       RowBox[{"\[ImaginaryI]", " ", 
        RowBox[{"sen", "(", 
         RowBox[{"-", 
          FractionBox["\[Pi]", "2"]}], ")"}]}]}], ")"}]}]}], 
   TraditionalForm]],ExpressionUUID->"ab1321c9-0574-5249-90e7-1aa874d125dd"],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "+", " ", 
    RowBox[{"\[ImaginaryI]", " ", "*", "\[ImaginaryI]"}], " ", "-", " ", 
    SuperscriptBox["\[ExponentialE]", 
