Cell[TextData[{
 "Soluci\[OAcute]n:\nLlamando ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "n"], "=", " ", 
    FractionBox["1", "n"]}], TraditionalForm]],ExpressionUUID->
  "4132cc9d-38bf-46da-bf17-4d8b623724d6"],
 ", el l\[IAcute]mite que debemos calcular se escribe como ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    FractionBox[
     RowBox[{
      RowBox[{
       RowBox[{
        FractionBox["1", "1"], "+", 
        FractionBox["1", "2"], "+"}], "..."}], "+", 
      FractionBox["1", "n"]}], "n"], "}"}], TraditionalForm]],ExpressionUUID->
  "392c5623-b23d-4f5d-860f-37874ca26ab4"],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    FractionBox[
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["a", "1"], " ", "+", " ", 
        SubscriptBox["a", "2"], " ", "+"}], " ", "..."}], " ", "+", " ", 
      SubscriptBox["a", "n"]}], "n"], "}"}], TraditionalForm]],ExpressionUUID->
  "09f0863e-910f-48cf-a01b-1b202c0a4c84"],
 ".\nSe trata de la media aritm\[EAcute]tica de los primeros n \
t\[EAcute]rminos de la sucesi\[OAcute]n {",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "n"], TraditionalForm]],ExpressionUUID->
  "982e36f9-fdda-4634-97f0-72cf7b269753"],
 "}. Por el Criterio de la media aritm\[EAcute]tica, como ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    SubscriptBox["a", "n"], "}"}], TraditionalForm]],ExpressionUUID->
  "79e71cab-edea-4179-a83e-886d58f4f5c7"],
 " \[LongRightArrow] 0 , ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    FractionBox[
     RowBox[{
      RowBox[{
       RowBox[{
        FractionBox["1", "1"], "+", 
        FractionBox["1", "2"], "+"}], "..."}], "+", 
      FractionBox["1", "n"]}], "n"], "}"}], TraditionalForm]],ExpressionUUID->
  "2724d743-bcce-43e2-a12c-58770e792446"],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    FractionBox[
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["a", "1"], " ", "+", " ", 
        SubscriptBox["a", "2"], " ", "+"}], " ", "..."}], " ", "+", " ", 
