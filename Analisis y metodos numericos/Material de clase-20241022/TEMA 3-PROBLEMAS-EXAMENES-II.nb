 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "n"], "=", " ", 
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox["1", "1"], "+", 
       FractionBox["1", "2"], "+"}], "..."}], "+", 
     FractionBox["1", "n"]}]}], TraditionalForm]],ExpressionUUID->
  "da3d562a-ed7b-43b7-a94c-9375fe708fc0"],
