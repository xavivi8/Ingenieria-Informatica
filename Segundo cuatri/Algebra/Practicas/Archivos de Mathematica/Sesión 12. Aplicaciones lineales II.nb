Cell[TextData[{
 "Calculamos bases del n\[UAcute]cleo y la imagen de la aplicaci\[OAcute]n \
lineal ",
 StyleBox["f",
  FontSlant->"Italic"],
 ": ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[DoubleStruckCapitalR]", "3"], TraditionalForm]],
  ExpressionUUID->"6c2aeca2-edad-44bc-8f73-1bc0f85d6dcc"],
 " \[LongRightArrow] ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[DoubleStruckCapitalR]", "4"], TraditionalForm]],
  ExpressionUUID->"d1fd64ab-337e-4ba1-b997-066f1433b7c6"],
 ", dada por ",
 StyleBox["f",
  FontSlant->"Italic"],
 "(",
 StyleBox["x ,y ,z",
  FontSlant->"Italic"],
 ") = (2",
 StyleBox["x - z",
  FontSlant->"Italic"],
 ", ",
 StyleBox["x",
  FontSlant->"Italic"],
 " + ",
 StyleBox["y",
  FontSlant->"Italic"],
 ", 3",
 StyleBox["x",
  FontSlant->"Italic"],
 " + y - ",
 StyleBox["z",
  FontSlant->"Italic"],
 ", 2",
 StyleBox["y",
  FontSlant->"Italic"],
 " + ",
 StyleBox["z",
  FontSlant->"Italic"],
 ")."
}], "Text",
 CellChangeTimes->{{3.7959214573678646`*^9, 3.795921523850625*^9}, {
