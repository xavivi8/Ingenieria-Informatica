Cell[BoxData[
 StyleBox[
  RowBox[{
   RowBox[{"EULER", "[", "matrizadyacencia_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"A", ",", "i"}], "}"}], ",", "\n", "   ", 
     RowBox[{
      RowBox[{"Euler", "=", "True"}], ";", "\n", "   ", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"CONEXO", "[", "matrizadyacencia", "]"}], ",", "\n", "      ", 
        RowBox[{
         RowBox[{"A", "=", 
          RowBox[{"matrizadyacencia", ".", "matrizadyacencia"}]}], ";", "\n", 
         "      ", 
         RowBox[{"Do", "[", "\n", "         ", 
          RowBox[{
           RowBox[{
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"Mod", "[", 
                RowBox[{
                 RowBox[{"A", "[", 
                  RowBox[{"[", 
                   RowBox[{"i", ",", "i"}], "]"}], "]"}], ",", "2"}], "]"}], "==",
                "1"}], ",", 
              RowBox[{
               RowBox[{"Euler", "=", "False"}], ";", 
               RowBox[{"Break", "[", "]"}], ";"}]}], "]"}], ";"}], "\n", 
           "      ", ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", 
             RowBox[{"Length", "[", "matrizadyacencia", "]"}]}], "}"}]}], 
          "]"}], ";"}], "\n", "   ", ",", "\n", "      ", 
        RowBox[{
         RowBox[{"Euler", "=", "False"}], ";"}]}], "\n", "   ", "]"}], ";", 
      "\n", "   ", "Euler"}]}], "\n", "]"}]}],
  FormatType->StandardForm]], "Input",ExpressionUUID->"9ac68dbe-c3ad-41a2-\
b609-0d12bd54d9d7"]
}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Programa 7.20. Ciclo de Euler",
 FontColor->RGBColor[0, 0, 1]]], "Subsubsection",ExpressionUUID->"c5fdb899-\
f619-48f9-a516-bbf0c219eb78"],

Cell[TextData[{
 StyleBox["F=CONJUNTO DE LADOS DE UN GRAFO NO ORIENTADO DE EULER;",
  FontWeight->"Bold"],
 "\n\ntempF=F;\nlistaciclos={};\nWhile[tempF\[NotEqual]{},\n    camino={};\n  \
  inicio=tempF[[1,1]];\n    otro=tempF[[1,2]];\n    \
AppendTo[camino,tempF[[1,1]]];\n    AppendTo[camino,tempF[[1,2]]];\n    \
tempF=Complement[tempF,{tempF[[1]]}];\n    While[otro\[NotEqual]inicio,\n     \
 j=1;\n      ultimo=Last[camino];\n      While[tempF[[j,1]]\[NotEqual]ultimo \
&& tempF[[j,2]]\[NotEqual]ultimo ,j++];\n      \
otro=Complement[{tempF[[j]][[1]],tempF[[j]][[2]]},{ultimo}][[1]];\n      \
AppendTo[camino,otro];\n      tempF=Complement[tempF,{tempF[[j]]}];\n      ];\
\n    AppendTo[listaciclos,camino];\n    ];\nnuevociclo=listaciclos[[1]];\n\
While[Length[listaciclos]\[NotEqual]1,\n    i=2;\n    \
While[Intersection[listaciclos[[1]],listaciclos[[i]]]\[Equal]{},i++];\n    \
opcion=Intersection[listaciclos[[1]],listaciclos[[i]]][[1]];\n    \
k1=Position[listaciclos[[1]],opcion][[1]][[1]];\n    \
k2=Position[listaciclos[[i]],opcion][[1]][[1]];\n    \
nuevociclo=listaciclos[[1]];\n    t=1;\n    \
Do[nuevociclo=Insert[nuevociclo,listaciclos[[i]][[cont]],k1+t];t++;,{cont,k2+\
1,Length[listaciclos[[i]]]}];\n    \
Do[nuevociclo=Insert[nuevociclo,listaciclos[[i]][[cont]],k1+t];t++;,{cont,2,\
k2}];\n    listaciclos=Delete[listaciclos,i];\n    \
listaciclos=Delete[listaciclos,1];\n    AppendTo[listaciclos,nuevociclo];\n   \
 ];\nnuevociclo"
}], "Program",
 CellChangeTimes->{{3.545672693953125*^9, 
  3.54567271690625*^9}},ExpressionUUID->"75179dd1-6777-4db6-be4b-\
d3c89ddaf7fa"]
}, Open  ]],

