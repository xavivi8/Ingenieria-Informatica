Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"GramSchmidt", "[", 
    RowBox[{"base_", ",", "p_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "baseortogonal", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"baseortogonal", "=", 
       RowBox[{"{", 
        RowBox[{"base", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Do", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"AppendTo", "[", 
          RowBox[{"baseortogonal", ",", 
           RowBox[{
            RowBox[{"base", "[", 
             RowBox[{"[", 
              RowBox[{"i", "+", "1"}], "]"}], "]"}], "-", 
            RowBox[{"Sum", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"(", 
                RowBox[{
                 RowBox[{"p", "[", 
                  RowBox[{
                   RowBox[{"base", "[", 
                    RowBox[{"[", 
                    RowBox[{"i", "+", "1"}], "]"}], "]"}], ",", 
                   RowBox[{"baseortogonal", "[", 
                    RowBox[{"[", "j", "]"}], "]"}]}], "]"}], "/", 
                 RowBox[{"p", "[", 
                  RowBox[{
                   RowBox[{"baseortogonal", "[", 
                    RowBox[{"[", "j", "]"}], "]"}], ",", 
                   RowBox[{"baseortogonal", "[", 
                    RowBox[{"[", "j", "]"}], "]"}]}], "]"}]}], ")"}], "*", 
               RowBox[{"baseortogonal", "[", 
                RowBox[{"[", "j", "]"}], "]"}]}], ",", 
              RowBox[{"{", 
               RowBox[{"j", ",", "i"}], "}"}]}], "]"}]}]}], "]"}], ";"}], " ", 
        RowBox[{"(*", 
         RowBox[{
          RowBox[{"M\[EAcute]todo", " ", "de", " ", "Gram"}], "-", 
          "Schmidt"}], "*)"}], "\[IndentingNewLine]", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", 
          RowBox[{
           RowBox[{"Length", "[", "base", "]"}], "-", "1"}]}], "}"}]}], "]"}],
       ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "baseortogonal", "]"}], ";"}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.7959428757324286`*^9, 3.7959428879147425`*^9}, {
