Cell["\<\
Usando esta t\[EAcute]cnica podemos programar una funci\[OAcute]n que calcule \
el rango :\
\>", "Text",
 CellChangeTimes->{{3.7937010844494157`*^9, 3.7937011652795377`*^9}, {
  3.793767082136507*^9, 
  3.7937670825513964`*^9}},ExpressionUUID->"2b42b96b-809b-4f38-a0a1-\
7feea3056b89"],

Cell[BoxData[
 RowBox[{
  RowBox[{"rango", "[", "A_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"n", ",", "m", ",", "s", ",", "k"}], "}"}], ",", 
    "\[IndentingNewLine]", "\t\t", 
    RowBox[{
     RowBox[{"n", "=", 
      RowBox[{
       RowBox[{"Dimensions", "[", "A", "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", "\t\t", 
     RowBox[{"m", "=", 
      RowBox[{
       RowBox[{"Dimensions", "[", "A", "]"}], "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", "\t\t", 
     RowBox[{"k", "=", 
      RowBox[{"Min", "[", 
       RowBox[{"n", ",", "m"}], "]"}]}], ";", "\[IndentingNewLine]", "\t\t", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"n", "\[Equal]", "m"}], " ", "&&", " ", 
        RowBox[{
         RowBox[{"Det", "[", "A", "]"}], "\[NotEqual]", "0"}]}], ",", " ", 
       "\[IndentingNewLine]", "                             ", 
       RowBox[{"Print", "[", 
        RowBox[{"\"\<El rango de esta matriz es \>\"", ",", " ", "k"}], "]"}],
        ",", "\[IndentingNewLine]", "                              ", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"i", "=", "k"}], ",", " ", 
         RowBox[{"i", "\[GreaterEqual]", "1"}], ",", 
         RowBox[{"i", "--"}], ",", 
         RowBox[{
          RowBox[{"s", "=", 
           RowBox[{"Minors", "[", 
            RowBox[{"A", ",", "i"}], "]"}]}], ";", "         ", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"s", "\[NotEqual]", 
             RowBox[{"Table", "[", 
              RowBox[{"0", ",", 
               RowBox[{"{", 
                RowBox[{"j1", ",", 
                 RowBox[{
                  RowBox[{"Dimensions", "[", "s", "]"}], "[", 
                  RowBox[{"[", "1", "]"}], "]"}]}], "}"}], ",", 
               RowBox[{"{", 
                RowBox[{"j2", ",", 
                 RowBox[{
                  RowBox[{"Dimensions", "[", "s", "]"}], "[", 
                  RowBox[{"[", "2", "]"}], "]"}]}], "}"}]}], "]"}]}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"Print", "[", "i", "]"}], ";", 
             RowBox[{"Break", "[", "]"}]}]}], " ", "]"}]}]}], "]"}]}], 
      "\[IndentingNewLine]", "]"}]}]}], "]"}]}]], "Input",ExpressionUUID->\
"81451d2d-a72c-45bb-8b38-2061fdb3cfdc"],

