MVI b 00 ; b dividendo
MVI c 00 ; c divisor
MVI h 00  ; h cociente
MVI l 00 ; l resto
LDA 0100
CPI 00
JZ fin
MOV b a 
MOV l a 
LDA 0101
CPI 00
JZ fin
MOV c a
CPI 00
JZ fin
CMP b
JNZ dividendo_menor

DIVIDENDO_MENOR: MOV a h
MOV l b
CPI 00
JMP fin

DIVIDIR: mov a l
SUB c
mov l a

CONTADOR: INR h
MOV a l
CPI 00
JZ fin
JNZ resto

RESTO: MOV a l
CMP c
JC fin
JMP dividir

FIN: mov a l
STA 0103
MOV a h
STA 0102
HLT