MVI e 00
MVI b 00
MVI c FE
MVI l 00
MVI h 00
MVI d 00

IN 00
MOV d a
IN 01
MOV e a
MVI a 11

INR h
LDAX b
CMP e
JZ ENCONTRADO
JMP buscar

ENCONTRADO: MOV l a
CALL NUM_DE_ENCUENTRO

BUSCAR: INR c
LDAX b
CMP e
JZ ENCONTRADO
DCR b
MOV a b
CPI 00
JZ fin

FIN: MOV a e
OUT 02
HLT

NUM_DE_ENCUENTRO: IN 02
INR a
OUT 02
RET