IN 00
MOV b a
IN 01
MOV d a

; Configurar lista
LXI h 00FE
MVI e 01
MVI l 00
MVI a ff

; Busqueda
BUSQUEDA: MOV a m
CMP d
JNZ siguiente
MOV c e
INR l

SIGUIENTE:INX h
INR r
DCR b
JNZ busqueda

; Resultados
MOV a l
CPI 00
JZ no_existe

; Si hay coincidencias
MOV a c
OUT 02
MOV a l
OUT 03
HLT

NO_EXISTE: LXI h 00FE
MOV a b
MOV c a
MVI a 00
DAD h
MOV m d
INR c
MOV a c
OUT 00
MOV a c
OUT 02
MVI a 01
OUT 03
HLT
