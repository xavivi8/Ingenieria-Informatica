Cell["\<\
El siguiente programa calcula la forma normal de Hermite por filas siguiendo \
los pasos del algoritmo anterior y mostrando cada uno de los pasos y \
transformaciones elementales por filas que realiza:\
\>", "Text",
 CellChangeTimes->{{3.794113247626187*^9, 3.794113284105452*^9}, {
  3.794113669716447*^9, 3.7941136702664857`*^9}, {3.7941152493218327`*^9, 
  3.7941152703926334`*^9}},ExpressionUUID->"34a6093a-5ddd-4328-9908-\
069c0bd6e49c"],

Cell[CellGroupData[{

Cell["HERMITE[]", "Subsubsection",
 CellChangeTimes->{{3.7941661097166853`*^9, 
  3.79416613878697*^9}},ExpressionUUID->"bbdf3117-0940-4a6c-925c-\
0083b4c4b5d6"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"HERMITE", "[", 
    RowBox[{"matriz_", ",", "opciones_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "pivote", ",", "aux", ",", "col", ",", "i", ",", "j", ",", "t", ",", 
       "kt", ",", "Adib", ",", "A", ",", "transformaciones", ",", "n", ",", 
       "m", ",", "el1", ",", "el2", ",", "el3"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"A", "=", "matriz"}], ";", 
      RowBox[{"m", "=", 
       RowBox[{
        RowBox[{"Dimensions", "[", "A", "]"}], "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], ";", 
      RowBox[{"n", "=", 
       RowBox[{
        RowBox[{"Dimensions", "[", "A", "]"}], "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{"Definimos", " ", "las", " ", "matrices", " ", "elementales"}],
        "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"el1", "[", 
        RowBox[{"i_", ",", "j_"}], "]"}], ":=", 
       RowBox[{"Module", "[", 
        RowBox[{
         RowBox[{"{", "B", "}"}], ",", 
         RowBox[{
          RowBox[{"B", "=", 
           RowBox[{"IdentityMatrix", "[", "m", "]"}]}], ";", 
          RowBox[{
           RowBox[{"B", "\[LeftDoubleBracket]", 
            RowBox[{"i", ",", "i"}], "\[RightDoubleBracket]"}], "=", "0"}], 
          ";", 
          RowBox[{
           RowBox[{"B", "\[LeftDoubleBracket]", 
            RowBox[{"j", ",", "j"}], "\[RightDoubleBracket]"}], "=", "0"}], 
          ";", 
          RowBox[{
           RowBox[{"B", "\[LeftDoubleBracket]", 
            RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}], "=", "1"}], 
          ";", 
          RowBox[{
           RowBox[{"B", "\[LeftDoubleBracket]", 
            RowBox[{"j", ",", "i"}], "\[RightDoubleBracket]"}], "=", 
           RowBox[{"B", "\[LeftDoubleBracket]", 
            RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}]}], ";", 
          "B"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"el2", "[", 
        RowBox[{"i_", ",", "s_"}], "]"}], ":=", 
       RowBox[{"Module", "[", 
        RowBox[{
         RowBox[{"{", "B", "}"}], ",", 
         RowBox[{
          RowBox[{"B", "=", 
           RowBox[{"IdentityMatrix", "[", "m", "]"}]}], ";", 
          RowBox[{
           RowBox[{"B", "\[LeftDoubleBracket]", 
            RowBox[{"i", ",", "i"}], "\[RightDoubleBracket]"}], "=", "s"}], 
          ";", "B"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"el3", "[", 
        RowBox[{"i_", ",", "j_", ",", "t_"}], "]"}], ":=", 
       RowBox[{"Module", "[", 
        RowBox[{
         RowBox[{"{", "B", "}"}], ",", 
         RowBox[{
          RowBox[{"B", "=", 
           RowBox[{"IdentityMatrix", "[", "m", "]"}]}], ";", 
          RowBox[{
           RowBox[{"B", "\[LeftDoubleBracket]", 
            RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}], "=", "t"}], 
          ";", "B"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"transformaciones", "=", 
       RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"A", "=", "matriz"}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"opciones", "\[Equal]", "1"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Print", "[", 
          RowBox[{
           RowBox[{"Style", "[", 
            RowBox[{
            "\"\<C\[CapitalAAcute]LCULO DE LA FORMA NORMAL DE HERMITE POR \
FILAS DE \>\"", ",", 
             RowBox[{"{", "Bold", "}"}]}], "]"}], ",", 
           RowBox[{"A", "//", "MatrixForm"}]}], "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{
         "Print", "[", "\"\<1. Calculamos la matriz escalonada:\>\"", "]"}], 
         ";"}]}], "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"pivote", "=", "1"}], ";", 
      RowBox[{"t", "=", "1"}], ";", 
      RowBox[{"col", "=", "0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"pivote", "\[NotEqual]", "0"}], " ", ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"t", "<", "m"}], "&&", " ", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"t", "+", "col"}], ")"}], "\[LessEqual]", "n"}]}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{"opciones", "\[Equal]", "1"}], ",", 
              "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{"Print", "[", 
                RowBox[{
                "\"\<  Buscamos el pivote \>\"", ",", "t", ",", 
                 "\"\<\.ba:\>\""}], "]"}], ";"}]}], 
             RowBox[{"(*", 
              RowBox[{"Buscamos", " ", "siguiente", " ", "pivote"}], "*)"}], 
             "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
            RowBox[{"j", "=", 
             RowBox[{"t", "+", "col"}]}], ";", 
            RowBox[{"i", "=", "t"}], ";", "\[IndentingNewLine]", 
            RowBox[{"While", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"A", "[", 
                RowBox[{"[", 
                 RowBox[{"i", ",", "j"}], "]"}], "]"}], "==", "0"}], ",", 
              RowBox[{
               RowBox[{"If", "[", 
                RowBox[{
                 RowBox[{"i", "<", "m"}], ",", 
                 RowBox[{
                  RowBox[{"i", "++"}], ";"}], ",", 
                 RowBox[{
                  RowBox[{"If", "[", 
                   RowBox[{
                    RowBox[{"j", "<", "m"}], ",", 
                    RowBox[{
                    RowBox[{"i", "=", "t"}], ";", 
                    RowBox[{"j", "++"}], ";", 
