Cell["\<\
ElementoNeutro=\"No existe\";
Do[
    If [ElementoNeutro==\"No existe\",
        neutro=True;
        Do[
          If[TrueQ[op[G[[CONTADORi]],G[[CONTADORj]]]== \
op[G[[CONTADORj]],G[[CONTADORi]]]] && TrueQ[op[G[[CONTADORi]],G[[CONTADORj]]]\
\[Equal]G[[CONTADORj]]],,neutro=False;Break[]];
          ,{CONTADORj,1,Length[G]}];
        If[neutro,ElementoNeutro=G[[CONTADORi]];Break[]];
        ];
    ,{CONTADORi,1,Length[G]}];
Print[\"Elemento Neutro: \",ElementoNeutro]\
\>", "Program"]
