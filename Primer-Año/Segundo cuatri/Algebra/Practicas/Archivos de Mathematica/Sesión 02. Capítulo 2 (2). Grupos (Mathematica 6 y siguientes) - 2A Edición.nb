Cell[TextData[StyleBox["Programa 2.12. Grupos de orden peque\[NTilde]o",
 FontColor->RGBColor[0, 0, 1]]], "Subsubsection",
 CellChangeTimes->{{3.4512079771855*^9, 3.451207987388625*^9}}],

Cell["\<\
tabla = TABLA DE OPERACIONES;
n = Length[tabla];
tiempo = TimeUsed[];
listadefinitiva = {};
listatablas = {tabla};
G = Table[i, {i, n}];
While[listatablas != {},
  listatablastemp = {};
  Do[
   tabla = listatablas[[j]];
   cero = Position[tabla, 0];
   fin = Length[cero];
   cero = cero[[1]];
   rellenos = Complement[G, Union[tabla[[cero[[1]]]], \
Transpose[tabla][[cero[[2]]]]]];
   Do[
    nuevatabla = tabla;
    If[rellenos[[i]] == 1,
     nuevatabla[[cero[[1]], cero[[2]]]] = 1;
     nuevatabla[[cero[[2]], cero[[1]]]] = 1;
     ,
     nuevatabla[[cero[[1]], cero[[2]]]] = rellenos[[i]];
     ];
    If[fin == 1,
     AppendTo[listadefinitiva, nuevatabla];
     ,
     AppendTo[listatablastemp, nuevatabla];
     ];
    
    , {i, 1, Length[rellenos]}];
   , {j, 1, Length[listatablas]}];
  listatablas = listatablastemp;
  ];
Print[\"Tiempo empleado: \", TimeUsed[] - tiempo];
Length[listadefinitiva]\
\>", "Program",
 CellChangeTimes->{{3.451208037044875*^9, 3.451208058294875*^9}}]
}, Closed]],

