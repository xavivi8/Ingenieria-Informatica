  RowBox[{
   RowBox[{"20", " ", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{
      RowBox[{"-", "2"}], "k"}]]}], " ", "+", "20"}]],
  CellChangeTimes->{3.756796093677807*^9},ExpressionUUID->
  "1b9b222e-37a4-4d79-86e5-a592c6de1854"],
 ";\n\t\t10= ",
 Cell[BoxData[
  RowBox[{"20", " ", 
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{
     RowBox[{"-", "2"}], "k"}]]}]],
  CellChangeTimes->{3.756796093677807*^9},ExpressionUUID->
  "bc086865-34cf-4d56-950e-971c9e44a970"],
 "; -2k = log (1/2) ; k = - log(1/2)/2=log(2)/2\nCon lo cual, y(5)= ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"20", " ", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{
      RowBox[{"-", "5"}], "k"}]], " ", 
    RowBox[{"(", 
     RowBox[{"1", "+", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"5", "k", " "}]]}], ")"}]}], "=", " ", 
   RowBox[{
    RowBox[{"20", " ", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "5"}], "k", " "}]]}], "+", "20"}]}]],
  CellChangeTimes->{3.756796093677807*^9},ExpressionUUID->
  "257babde-ff1b-4621-8d3c-ed98ecac4aee"],
 " = 20 + 20  ",
 Cell[BoxData[
  SuperscriptBox["\[ExponentialE]", 
   RowBox[{
    RowBox[{"-", "5"}], "log", " ", 
    RowBox[{
     RowBox[{"(", "2", ")"}], "/", "2", " "}]}]]],
  CellChangeTimes->{3.756796093677807*^9},ExpressionUUID->
  "35403787-be2f-42c1-9cc6-b3d262c1fa42"],
 "= 20 + 20*",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["2", 
    RowBox[{
     RowBox[{"-", "5"}], "/", "2"}]], TraditionalForm]],ExpressionUUID->
  "89a4a3a9-c546-4370-b3ac-a3d08db7a171"],
 ", aproximadamente 23,5355"
}], "Text",
