Cell[TextData[{
 "Soluci\[OAcute]n:\nComenzamos calculando el cociente que aparece. Para ello \
multiplicamos y dividimos por el conjugado del denominador.\n",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"1", "+", 
     RowBox[{"3", "\[ImaginaryI]"}]}], 
    RowBox[{"1", "-", 
     RowBox[{"2", "\[ImaginaryI]"}]}]], TraditionalForm]],ExpressionUUID->
  "7127fabf-bb97-4b1a-84f3-1f1dff4bdeeb"],
 " = ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{"3", "\[ImaginaryI]"}]}], ")"}], 
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{"2", "\[ImaginaryI]"}]}], ")"}]}], 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{"2", "\[ImaginaryI]"}]}], ")"}], 
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{"2", "\[ImaginaryI]"}]}], ")"}]}]], TraditionalForm]],
  ExpressionUUID->"978987d3-f256-40b9-9a0a-6e84d711dd23"],
 " = ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"(", 
     RowBox[{"1", "+", 
      RowBox[{"5", "\[ImaginaryI]"}], "+", 
      RowBox[{"6", 
       SuperscriptBox["\[ImaginaryI]", "2"]}]}], ")"}], 
    RowBox[{"1", "+", "4"}]], TraditionalForm]],ExpressionUUID->
  "247a626d-00d6-4c5b-b5d5-068093f808f5"],
 "=",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"-", "5"}], "+", 
     RowBox[{"5", "\[ImaginaryI]"}]}], "5"], TraditionalForm]],ExpressionUUID->
  "b543e83a-f40e-41bc-bcf2-86d15a7a467e"],
 "= - 1 + \[ImaginaryI]   \[DoubleLongRightArrow]  ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     FractionBox[
      RowBox[{"1", "+", 
       RowBox[{"3", "\[ImaginaryI]"}]}], 
      RowBox[{"1", "-", 
       RowBox[{"2", "\[ImaginaryI]"}]}]], ")"}], "23"], TraditionalForm]],
  ExpressionUUID->"0dae7352-ea22-434d-a462-0197557d519a"],
 " = ",
