   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"1", "-", "\[ImaginaryI]"}], ")"}], "2"], TraditionalForm]],
  ExpressionUUID->"f65b510a-944e-494c-9179-e3fc528269b2"],
 " =",
