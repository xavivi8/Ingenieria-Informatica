    RowBox[{"b", "\[Rule]", "17"}], ",", 
    RowBox[{"c", "\[Rule]", 
     RowBox[{"-", 
      FractionBox["8", "3"]}]}], ",", 
    RowBox[{"d", "\[Rule]", 
