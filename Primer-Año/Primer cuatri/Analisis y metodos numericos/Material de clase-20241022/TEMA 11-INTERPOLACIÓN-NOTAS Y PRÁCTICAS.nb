     RowBox[{"p", "'"}], "[", "4", "]"}], "\[Equal]", "1"}], ",", 
   RowBox[{
    RowBox[{"p", "[", "7", "]"}], "\[Equal]", 
    RowBox[{"-", "3"}]}], ",", 
   RowBox[{
    RowBox[{
     RowBox[{"p", "'"}], "[", "7", "]"}], "\[Equal]", 
