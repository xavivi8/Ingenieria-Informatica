 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"TABLADEVERDAD", "[", 
     RowBox[{"funcion_", ",", "nombres_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "n", ",", "x", ",", "j", ",", "f", ",", "tabla", ",", "resto"}], "}"}],
